h1,
.h1 {
    font-family: Interstate-Light, Arial, "Helvetica Neue", Helvetica, sans-serif;
    font-weight: 200;
    font-size: 26px;
    line-height: 1.2;
    color: #222222;
    letter-spacing: -.02em;
    margin-top: 12px;
    margin-bottom: 12px;
    -webkit-font-smoothing: antialiased;
    -moz-osx-font-smoothing: grayscale;
}
h1 small,
.h1 small,
h1 .small,
.h1 .small {
    font-size: 70%;
    font-weight: normal;
    line-height: 1;
    color: #666;
}
